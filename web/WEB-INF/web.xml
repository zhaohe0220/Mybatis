        <servlet-class>com.mybatis.servlet.Update</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>update</servlet-name>
        <url-pattern>/update.action</url-pattern>
    </servlet-mapping>
</web-app>