    </servlet>
    <servlet-mapping>
        <servlet-name>addMessage</servlet-name>
        <url-pattern>/addMessage.action</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>insert</servlet-name>
        <servlet-class>com.mybatis.servlet.Insert</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>insert</servlet-name>
        <url-pattern>/insert.action</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>editMessage</servlet-name>
        <servlet-class>com.mybatis.servlet.editMessage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>editMessage</servlet-name>
